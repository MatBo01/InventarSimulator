                <MenuItem fx:id="miName" mnemonicParsing="false" text="Action 1" />
                <MenuItem mnemonicParsing="false" text="Action 2" />
              </items>
            </MenuButton>
            <Button fx:id="btTest" layoutX="194.0" layoutY="13.0" mnemonicParsing="false" onAction="#handleButtonDbAuslesenAction" text="Anzeigen" />
         </children>
      </Pane>
      <TableView fx:id="tvWaffen" prefHeight="801.0" prefWidth="1398.0">
        <columns>
          <TableColumn fx:id="tcName" prefWidth="174.0" text="Name" />
          <TableColumn fx:id="tcWaffenArt" prefWidth="174.0" text="Art" />
            <TableColumn fx:id="tcSeltenheit" prefWidth="174.0" text="Seltenheit" />
            <TableColumn fx:id="tcWert" prefWidth="174.0" text="Wert" />
            <TableColumn fx:id="tcStaerke" prefWidth="174.0" text="Angriff" />
